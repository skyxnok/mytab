{
  "manifest_version": 3,
  "name": "myTab",
  "version": "1.0",
  "permissions": ["bookmarks", "storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "add.html"
  }
}